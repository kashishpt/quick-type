{
  "name": "quick-type",
  "displayName": "Quick type",
  "description": "",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:quick-type.addDoubleQuotes",
    "onCommand:quick-type.addSingleQuotes",
    "onCommand:quick-type.addTickQuotes",
    "onCommand:quick-type.toMap",
    "onCommand:quick-type.toArray"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "quick-type.addDoubleQuotes",
        "title": "Convert words to strings (\"string\")",
        "category": "Strings"
      },
      {
        "command": "quick-type.addSingleQuotes",
        "title": "Convert words to strings ('string')",
        "category": "Strings"
      },
      {
        "command": "quick-type.addTickQuotes",
        "title": "Convert words to strings (`string`)",
        "category": "Strings"
      },
      {
        "command": "quick-type.toMap",
        "title": "Convert rows of space separated key-value pairs to a map",
        "category": "Data structures"
      },
      {
        "command": "quick-type.toArray",
        "title": "Convert whitespace separated values to an array",
        "category": "Data structures"
      }
    ],
    "configuration": [
      {
        "title": "Regex",
        "properties": {
          "quick-type.stringRegex": {
            "type": "string",
            "default": "^(\\w|-)+",
            "description": "Regex for matching words in string functions"
          },
          "quick-type.keyValueRegex": {
            "type": "string",
            "default": "^\\s*([\\w-]+|'.+'|`.+`|\".+\")",
            "description": "Regex for matching values in data structure functions"
          }
        }
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "@vscode/test-electron": "^2.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kashishpt/quick-type.git"
  },
  "publisher": "testudostail"
}
